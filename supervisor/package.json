{
  "name": "wmd-supervisor",
  "version": "0.0.1",
  "private": true,
  "main": "supervisor.js",
  "scripts": {
    "start": "meteor run",
    "lint": "eslint ./lib ./client ./server --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha .scripts/mocha_boot.js client/**/tests/**/*.js --compilers js:babel-core/register",
    "test": "npm run lint && npm run testonly",
    "test-meteor": "meteor test-app --driver-package practicalmeteor:mocha",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx"
  },
  "devDependencies": {
    "babel-core": "6.x.x",
    "babel-eslint": "^6.0.2",
    "babel-plugin-react-require": "2.x.x",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-meteor": "^6.6.6",
    "babel-preset-react": "6.x.x",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "^2.0.0",
    "eslint": "^2.6.0",
    "eslint-config-react": "^1.1.1",
    "eslint-plugin-jsx-control-statements": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "jsdom": "^8.0.4",
    "jsx-control-statements": "^3.1.0",
    "mocha": "2.x.x",
    "react-addons-test-utils": "^15.0.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.2",
    "sinon": "1.17.x"
  },
  "dependencies": {
    "ansi_up": "^1.3.0",
    "dockerode": "^2.2.10",
    "domready": "^1.0.8",
    "formsy-react": "^0.17.0",
    "mantra-core": "^1.2.0",
    "material-ui": "^0.15.0-alpha.2",
    "meteor-node-stubs": "~0.2.0",
    "normalize.css": "^4.0.0",
    "react": "^15.0.1",
    "react-addons-create-fragment": "^15.0.1",
    "react-addons-css-transition-group": "^15.0.1",
    "react-addons-linked-state-mixin": "^15.0.1",
    "react-addons-perf": "^15.0.1",
    "react-addons-pure-render-mixin": "^15.0.1",
    "react-addons-test-utils": "^15.0.1",
    "react-addons-transition-group": "^15.0.1",
    "react-addons-update": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-forms-ui": "^0.3.2",
    "react-komposer": "^1.4.1",
    "react-mounter": "^1.0.0",
    "react-redux": "^4.4.1",
    "react-tap-event-plugin": "^0.2.2",
    "react-toolbox": "^0.15.1",
    "react-transform-hmr": "^1.0.4",
    "recompose": "^0.16.0",
    "redbox-react": "^1.2.3",
    "redux": "^3.3.1",
    "redux-form": "^5.0.1",
    "tether": "^1.2.0"
  },
  "cssModules": {
    "extensions": [
      "scss"
    ],
    "globalVariables": [
      "node_modules/react-toolbox/lib/_colors.scss",
      {
        "theme-building": true
      },
      "client/toolbox-theme.scss"
    ],
    "outputJsFilePath": "{dirname}/{basename}",
    "explicitIncludes": [
      "node_modules/react-toolbox"
    ]
  }
}
